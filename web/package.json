{
  "name": "ortho-implant-co-marketing-website",
  "private": true,
  "description": "Gatsby TypeScript Starter with Styled System",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "cleancache": "gatsby clean",
    "develop": "gatsby develop",
    "dev": "gatsby develop -o",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "ssr": "gatsby build && gatsby serve",
    "lint": "tslint -t stylish -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix",
    "format": "prettier --write content/**/*.yaml",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
    "@types/lodash": "^4.14.172",
    "babel-plugin-styled-components": "^1.13.2",
    "gatsby": "^2.20.7",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-google-tagmanager": "^3.10.0",
    "gatsby-plugin-gtag": "^1.0.13",
    "gatsby-plugin-linkedin-insight": "^1.0.1",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-netlify": "^2.11.1",
    "gatsby-plugin-nprogress": "^2.10.0",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.4",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-plugin-twitter-pixel": "^2.0.2",
    "gatsby-plugin-typescript": "^2.12.1",
    "gatsby-source-filesystem": "^2.11.1",
    "gatsby-source-sanity": "^5.0.6",
    "gatsby-transformer-sharp": "^2.12.1",
    "lodash": "^4.17.21",
    "polished": "^3.7.2",
    "react": "^16.14.0",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.14.0",
    "react-helmet": "^5.2.1",
    "react-id-swiper": "^3.0.0",
    "react-image-lightbox": "^5.1.4",
    "react-intersection-observer": "^8.32.0",
    "react-responsive-embed": "^2.1.0",
    "react-scroll-parallax": "^2.4.0",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.1",
    "styled-css-grid": "^1.2.1",
    "styled-system": "^4.2.4",
    "swiper": "^5.4.2",
    "typescript": "^3.9.10"
  },
  "devDependencies": {
    "@types/node": "^10.17.60",
    "@types/react": "^16.14.11",
    "@types/react-dom": "^16.9.14",
    "@types/react-helmet": "^5.0.17",
    "@types/styled-components": "^4.4.3",
    "@types/styled-system": "^4.2.2",
    "cross-env": "^5.2.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.13.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caseykennedy/oic"
  },
  "bugs": {
    "url": "https://github.com/caseykennedy/oic/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.yaml": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ]
  }
}
