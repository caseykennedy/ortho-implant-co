{
  "name": "ortho-implant-co-marketing-website",
  "private": true,
  "description": "Gatsby TypeScript Starter with Styled System",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env gatsby build",
    "cleancache": "gatsby clean",
    "develop": "gatsby develop",
    "dev": "gatsby develop -o",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "ssr": "gatsby build && gatsby serve",
    "lint": "tslint -t stylish -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix",
    "format": "prettier --write content/**/*.yaml",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
    "babel-plugin-styled-components": "^1.13.2",
    "gatsby": "^4.24.5",
    "gatsby-plugin-google-tagmanager": "^4.25.0",
    "gatsby-plugin-image": "^2.5.2",
    "gatsby-plugin-manifest": "^4.5.2",
    "gatsby-plugin-netlify": "^4.0.0",
    "gatsby-plugin-nprogress": "^4.5.0",
    "gatsby-plugin-offline": "^5.22.0",
    "gatsby-plugin-react-helmet": "^5.5.0",
    "gatsby-plugin-sanity-image": "^0.6.0",
    "gatsby-plugin-sharp": "^4.5.2",
    "gatsby-plugin-sitemap": "^5.5.0",
    "gatsby-plugin-styled-components": "^5.5.0",
    "gatsby-plugin-typescript": "^4.5.2",
    "gatsby-source-filesystem": "^4.22.0",
    "gatsby-source-sanity": "^7.0.4",
    "gatsby-transformer-sharp": "^4.22.0",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-id-swiper": "^3.0.0",
    "react-image-lightbox": "^5.1.4",
    "react-intersection-observer": "^8.32.0",
    "react-netlify-forms": "^1.3.3",
    "react-responsive-embed": "^2.1.0",
    "react-scroll-parallax": "^2.4.0",
    "react-spring": "^8.0.27",
    "sharp": "^0.30.7",
    "styled-components": "^5.3.5",
    "styled-system": "^4.2.4",
    "swiper": "^5.4.2",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^17.0.29",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "@types/styled-system": "^4.2.2",
    "cross-env": "^5.2.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.13.1",
    "tsconfig-paths": "^4.2.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caseykennedy/oic"
  },
  "bugs": {
    "url": "https://github.com/caseykennedy/oic/issues"
  },
  "lint-staged": {
    "*.yaml": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ]
  }
}
